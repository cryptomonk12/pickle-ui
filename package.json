{
  "name": "pickle-jar",
  "version": "0.1.0",
  "main": "index.js",
  "author": "BigBrainBriner <bigbrainbriner@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "yarn typechain && next",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "format": "prettier --write \"./**/*.{json,js,jsx,ts,tsx}\" --ignore-path .gitignore",
    "postinstall": "yarn typechain & typesync",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
    "tsc": "tsc",
    "typechain": "typechain --target ethers-v5 --outDir ./containers/Contracts './containers/ABIs/*.json'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "yarn build && aws s3 sync build/ s3://pickle-finance/"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.0.4",
    "@geist-ui/react": "^2.0.2",
    "@geist-ui/react-icons": "^1.0.0",
    "@primer/octicons-react": "^11.0.0",
    "@studydefi/money-legos": "^2.3.7",
    "ethers": "^5.0.14",
    "ethers-multicall": "^0.1.3",
    "moment": "^2.29.1",
    "next": "^9.5.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^16.13.1",
    "react-jazzicon": "^0.1.3",
    "rxjs": "^6.6.3",
    "sass": "^1.26.11",
    "styled-components": "^5.2.0",
    "unstated-next": "^1.1.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "clsx": "^1.1.1",
    "fontsource-roboto": "^4.0.0",
    "node-fetch": "^2.6.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "recharts": "^1.8.5",
    "recharts-scale": "^0.4.3",
    "typeface-source-code-pro": "^1.1.13",
    "web3": "^1.3.0"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^1.0.0",
    "@types/eslint": "^7.2.6",
    "@types/node": "^14.11.1",
    "@types/node-fetch": "^2.5.8",
    "@types/prettier": "2.1.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^17.0.0",
    "@types/react-is": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/recharts": "^1.8.19",
    "@types/recharts-scale": "^1.0.0",
    "@types/sass": "^1.16.0",
    "@types/styled-components": "^5.1.3",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "2.1.2",
    "typechain": "^2.0.0",
    "typescript": "^4.0.3",
    "typesync": "^0.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn format"
    ]
  },
  "private": true,
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
